<nav class="breadcrumbs-container">
	<div class="nav-wrapper">
		<div class="col s12">
			<%= link_to root_path, class: 'breadcrumb' do %>
				<i class="material-icons before">home</i>HOME
			<% end %>
			<%= link_to '店舗一覧', admin_shops_path, class: "breadcrumb" %>
			<a class="breadcrumb active">店舗登録</a>
		</div>
	</div>
</nav>

<div class="container">
	<h2 class="title">【管理者】店舗登録</h2>
  <div class="card devise-card">
    <div class="form-wrap">
			<%= form_with model: @shop, local: true, url: {action: :create} do |f| %>
				<%= render partial: 'form', locals: {f: f} %>
			<% end %>
		</div>
		<p class="devise-link">
			<%= link_to 'お店の一覧を表示する', shops_path, class: 'nav-link' %>
		</p>
  </div>
</div>
