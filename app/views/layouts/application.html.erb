<!DOCTYPE html>
<html>
  <head>
    <title>Cafedotcom</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css' %>
    <%= stylesheet_link_tag 'https://unpkg.com/swiper/css/swiper.min.css' %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_include_tag 'https://unpkg.com/swiper/js/swiper.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/lazysizes/5.2.0/lazysizes.min.js' %>
  </head>
  <body>
    <div>
      <%= render 'layouts/navbar' %>
      <%= render 'layouts/toast' %>
      <div>
        <%= yield %>
      </div>
    </div>
    <div id="page_top"><a href="#"></a></div>
    <%= render 'layouts/footer' %>
  </body>
</html>
